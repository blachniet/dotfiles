{
    "[html]": {
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2
    },
    "[vue]": {
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "editor.fontSize": 14,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        100
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "t",
                "p"
            ],
            "commands": [
                "go.test.package"
            ]
        }
    ],
    "vim.leader": ",",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "workbench.sideBar.location": "right",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "git.autofetch": true,
    "files.associations": {
        "Vagrantfile": "ruby"
    }
}